    "public.users_with_tokens": {
      "columns": {
        "slackId": {
          "name": "slackId",
          "type": "text",
          "primaryKey": true,
          "notNull": true
        },
        "avatarUrl": {
          "name": "avatarUrl",
          "type": "text",
          "primaryKey": false,
          "notNull": true
        },
